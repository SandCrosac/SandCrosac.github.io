<script type="application/javascript">
  $(function (){
      $.getJSON("db.json", function (data){
        var strHtml = "123";
        //清空内容
        $.each(data, function (infoIndex, info){
          strHtml += "姓名：" + info["name"] + "<br>";
          strHtml += "性别：" + info["sex"] + "<br>";
          strHtml += "邮箱：" + info["email"] + "<br>";
          strHtml += "<hr>"
        })
        console.log(strHtml);
        //显示处理后的数据
      })
  })
</script>